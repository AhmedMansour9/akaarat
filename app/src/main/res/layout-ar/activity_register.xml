                <RelativeLayout
                    android:layout_width="@dimen/_90sdp"
                    android:id="@+id/relative_owner"
                    android:layout_marginStart="@dimen/_30sdp"
                    android:background="@drawable/bc_images"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_height="@dimen/_80sdp">

                    <ImageView
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:src="@drawable/ic_owner"
                        android:id="@+id/img_owner"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_20sdp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/img_owner"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:text="@string/ownerrealstate"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="#000"

                        />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="@dimen/_90sdp"
                    android:id="@+id/relative_owner_selected"
                    android:visibility="invisible"
                    android:layout_marginStart="@dimen/_30sdp"
                    android:background="@drawable/bc_images_selected"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_height="@dimen/_80sdp">

                    <ImageView
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:src="@drawable/ownerselected"
                        android:id="@+id/img_owner_selected"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_20sdp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/img_owner_selected"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:text="@string/ownerrealstate"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="#000"
                        />
                </RelativeLayout>



