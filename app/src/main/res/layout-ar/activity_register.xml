                <RelativeLayout
                    android:layout_width="@dimen/_90sdp"
                    android:id="@+id/relative_tenent"
                    android:background="@drawable/bc_images"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_30sdp"
                    android:layout_alignParentLeft="true"
                    android:layout_height="@dimen/_80sdp">

                    <ImageView
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:src="@drawable/ic_rent"
                        android:id="@+id/img_rent"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_20sdp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/img_rent"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:text="@string/tenant"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="#000"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="@dimen/_90sdp"
                    android:id="@+id/relative_tenent_selected"
                    android:layout_marginEnd="@dimen/_30sdp"
                    android:background="@drawable/bc_images_selected"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_alignParentLeft="true"
                    android:visibility="invisible"
                    android:layout_height="@dimen/_80sdp">

                    <ImageView
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:src="@drawable/ic_rent"
                        android:id="@+id/img_rent_selected"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_20sdp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/img_rent_selected"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:text="@string/tenant"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="#000"
                        />



                </RelativeLayout>



        <EditText
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/bs_spinner_search"
            android:hint="@string/username"
            android:layout_below="@+id/relative_owner"
            android:textColorHint="#bcbcbc"
            android:id="@+id/E_User_Name"
            android:paddingStart="@dimen/_15sdp"
            android:textSize="@dimen/_13sdp"
            android:textColor="#bcbcbc"

            />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/bs_spinner_search"
            android:hint="@string/email"
            android:layout_below="@+id/E_User_Name"
            android:textColorHint="#bcbcbc"
            android:inputType="textEmailAddress"
            android:id="@+id/E_Email"
            android:paddingStart="@dimen/_15sdp"
            android:textSize="@dimen/_13sdp"
            android:textColor="#bcbcbc"

            />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/bs_spinner_search"
            android:hint="@string/phone"
            android:layout_below="@+id/E_Email"
            android:textColorHint="#bcbcbc"
            android:inputType="number"
            android:id="@+id/E_Phone"
            android:paddingStart="@dimen/_15sdp"
            android:textSize="@dimen/_13sdp"
            android:textColor="#bcbcbc"

            />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/bs_spinner_search"
            android:hint="@string/national"
            android:layout_below="@+id/E_Phone"
            android:textColorHint="#bcbcbc"
            android:inputType="number"
            android:id="@+id/E_NoationlId"
            android:paddingStart="@dimen/_15sdp"
            android:textSize="@dimen/_13sdp"
            android:textColor="#bcbcbc"
            />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/rel_types"
            android:layout_below="@+id/E_NoationlId"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/bs_spinner_search">

            <androidx.appcompat.widget.AppCompatSpinner
                android:id="@+id/Tybes_Spinner"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp"
                android:spinnerMode="dialog"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
                app:backgroundTint="#cdcdcd">


            </androidx.appcompat.widget.AppCompatSpinner>


        </RelativeLayout>


        <EditText
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/bs_spinner_search"
            android:hint="@string/password"
            android:layout_below="@+id/rel_types"
            android:textColorHint="#bcbcbc"
            android:inputType="textPassword"
            android:id="@+id/E_Password"
            android:paddingStart="@dimen/_15sdp"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:layout_below="@+id/E_Password"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="#0591de"
            android:text="@string/register"
            android:textSize="@dimen/_16sdp"
            android:textStyle="bold"
            android:textAllCaps="false"
            android:id="@+id/Sign_Up"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Sign_Up"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_30sdp"
            android:textColor="#cbcfd2"
            android:text="@string/alreadyhaveaccount"
            android:textStyle="bold"
            android:textSize="@dimen/_15sdp"
            android:id="@+id/already"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Sign_Up"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:textColor="#38849e"
            android:text="@string/signnin"
            android:id="@+id/Sign_In"
            android:textStyle="bold"
            android:textSize="@dimen/_15sdp"
            android:layout_toLeftOf="@+id/already"
            />
                <ProgressBar
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:id="@+id/progross"
                    android:layout_centerInParent="true"
                    android:visibility="gone"
                    />
    </RelativeLayout>
        </RelativeLayout>

    </ScrollView>
</RelativeLayout>